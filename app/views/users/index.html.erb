<div class="container">
<div class="row">
  <div class="col-md-3">
    <%= render 'books/user', user: current_user, book: @new_book %>
  </div>
<div class="col-md-8 offset-md-1">
<h1>Users</h1>
<table class="table table-hover table-inverse">
    <thread>
        <th>image</th>
        <th>name</th>
        <th></th>
    </thread>
    <% @users.each do |user| %>
    <tbody>
        <td>
            <%= attachment_image_tag user, :profile_image, :fill, 10,10, fallback: "no_image.jpg"  %>
        </td>
        <td><%= user.name %></td>
        <td><%= link_to "Show", user_path(user.id) %></td>
    </tbody>
    <% end %>
</table>
  </div>
</div>
</div>